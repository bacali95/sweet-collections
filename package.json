{
  "name": "sweet-collections",
  "version": "1.1.0",
  "description": "Different Javascript collections written in Typescript",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "repository": {
    "url": "https://github.com/bacali95/sweet-collections.git",
    "type": "git"
  },
  "author": {
    "name": "Nasreddine Bac Ali",
    "email": "nasreddine.bacali95@gmail.com",
    "url": "https://nasreddinebacali.info"
  },
  "keywords": [
    "collections",
    "algorithms",
    "lrumap",
    "lfumap",
    "sortedmap",
    "lruset",
    "lfuset",
    "sortedset",
    "sortedarray",
    "heap"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "rimraf lib && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc --module esnext --declaration --outDir lib/esm",
    "benchmark": "ts-node benchmark",
    "lint": "eslint .",
    "prettier": "prettier --list-different '**/{*,.*}.ts'",
    "prettier:fix": "prettier --write --list-different '**/{*,.*}.ts'",
    "test": "jest --coverage --no-cache && make-coverage-badge"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "benny": "^3.7.1",
    "chalk": "^4.1.2",
    "collections": "^5.1.13",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^28.1.3",
    "lru-cache": "^7.14.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
